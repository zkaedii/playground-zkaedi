# Private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Arbiscan API key for verification
ARBISCAN_API_KEY=your_arbiscan_api_key

# Deployment parameters (V2 & V3)
TOKEN_NAME="UUPS Token"
TOKEN_SYMBOL="UUPS"
INITIAL_SUPPLY=1000000000000000000000000    # 1M tokens (18 decimals)
MAX_SUPPLY=10000000000000000000000000       # 10M tokens (V3 only)
BURN_RATE=50                                # 0.5% in basis points

# Upgrade parameters
PROXY_ADDRESS=0x...                         # Existing proxy to upgrade
